.PHONY: clean All

All:
	@echo "----------Building project:[ JohnnyEngine - Release ]----------"
	@"$(MAKE)" -f  "JohnnyEngine.mk"
	@echo "----------Building project:[ JohnnyEngine-Template - Release ]----------"
	@"$(MAKE)" -f  "JohnnyEngine-Template.mk"
clean:
	@echo "----------Cleaning project:[ JohnnyEngine - Release ]----------"
	@"$(MAKE)" -f  "JohnnyEngine.mk"  clean
	@echo "----------Cleaning project:[ JohnnyEngine-Template - Release ]----------"
	@"$(MAKE)" -f  "JohnnyEngine-Template.mk" clean
